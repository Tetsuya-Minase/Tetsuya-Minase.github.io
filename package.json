{
  "name": "ng-my-page",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:static": "scully serve",
    "start:local": "run-s generate start:static",
    "build": "ng build --aot",
    "build:prod": "ng build --aot --configuration production",
    "test": "ng test",
